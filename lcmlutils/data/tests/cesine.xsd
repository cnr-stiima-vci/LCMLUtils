                     <xs:attribute name="min">
                        <xs:simpleType>
                           <xs:restriction base="xs:decimal"/>
                        </xs:simpleType>
                     </xs:attribute>
                     <xs:attribute name="max">
                        <xs:simpleType>
                           <xs:restriction base="xs:decimal"/>
                        </xs:simpleType>
                     </xs:attribute>
                  </xs:complexType>
               </xs:element>
               <xs:element maxOccurs="1" minOccurs="0" name="sequential_temporal_relationship_length_o_y">
                  <xs:complexType>
                     <xs:attribute name="min">
                        <xs:simpleType>
                           <xs:restriction base="xs:decimal"/>
                        </xs:simpleType>
                     </xs:attribute>
                     <xs:attribute name="max">
                        <xs:simpleType>
                           <xs:restriction base="xs:decimal"/>
                        </xs:simpleType>
                     </xs:attribute>
                  </xs:complexType>
               </xs:element>
               <xs:element maxOccurs="1" minOccurs="0" name="sequential_temporal_relationship_type" type="SeqTemporalRelationshipTypeEnum"/>
